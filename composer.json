{
  "name": "lostkobrakai/migrations",
  "description": "Module to handle migration files in ProcessWire",
  "homepage": "https://lostkobrakai.github.io/Migrations/",
  "keywords": [
    "ProcessWire",
    "migration",
    "database"
  ],
  "type": "pw-module",
  "license": "MIT",
  "bin": [
    "bin/migrate"
  ],
  "authors": [
    {
      "name": "Benjamin Milde",
      "homepage": "https://kobrakai.de/",
      "role": "Developer"
    }
  ],
  "require": {
    "hari/pw-module": "~1.0",
    "symfony/console": "^3.1"
  },
  "require-dev": {
    "crysalead/kahlan": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "ProcessWire\\Migrations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "spec/"
    },
    "classmap": [
      "tests/wire/core",
      "tests/site/modules/Migrations"
    ],
    "files": [
      "tests/wire/core/Functions.php",
      "tests/wire/core/Interfaces.php",
      "tests/wire/core/LanguageFunctions.php"
    ],
    "exclude-from-classmap": [
      "tests/wire/core/compat2x/"
    ]
  }
}
